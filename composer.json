{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The skeleton application for the Laravel framework.",
    "keywords": ["laravel", "framework"],
    "license": "MIT",
    "require": {
        "php": "^8.2",
        "ext-gd": "*",
        "ext-imagick": "*",
        "dede/hydrastorage": "^0.5.2",
        "fakerphp/faker": "^1.9.1",
        "guzzlehttp/guzzle": "^7.2",
        "laravel-notification-channels/telegram": "^5.0",
        "laravel/framework": "^10.10",
        "laravel/sanctum": "^3.3",
        "laravel/slack-notification-channel": "^3.2",
        "laravel/telescope": "^5.1",
        "laravel/tinker": "^2.8",
        "league/flysystem-aws-s3-v3": "^3.0",
        "mews/purifier": "^3.4",
        "predis/predis": "^2.2",
        "spatie/laravel-backup": "^9.1",
        "spatie/laravel-permission": "^6.4",
        "staudenmeir/eloquent-eager-limit": "^1.0",
        "stevebauman/location": "^7.3",
        "westacks/telebot-laravel": "^3.2"
    },
    "require-dev": {
        "larastan/larastan": "^2.0",
        "laravel/pint": "^1.0",
        "laravel/sail": "^1.18",
        "mockery/mockery": "^1.4.4",
        "nunomaduro/collision": "^7.0",
        "nunomaduro/phpinsights": "^2.11",
        "pestphp/pest": "^2.34",
        "pestphp/pest-plugin-faker": "^2.0",
        "pestphp/pest-plugin-laravel": "^2.3",
        "pestphp/pest-plugin-stressless": "^2.0",
        "phpstan/phpstan": "^1.12",
        "phpunit/phpunit": "^10.1",
        "spatie/laravel-ignition": "^2.0",
        "squizlabs/php_codesniffer": "*"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        },
        "files": [
            "app/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        },
        "files": [
            "app/helpers.php"
        ]
    },
    "repositories":[
        {
            "type": "vcs",
            "url": "https://github.com/Dede182/HydraStorage"
        }
    ],
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": [
                "barryvdh/laravel-ide-helper"
            ]
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "php-http/discovery": true,
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    }
}
